{
  "functions": {
    "api/**/*.php": {
      "runtime": "vercel-php@0.7.3"
    }
  },
  "redirects": [
    {
      "source": "/api/:path((?!.*\\.(exe|tmp)$).*)",
      "destination": "https://www.aazperfumes.com.br/",
      "permanent": false
    },
    {
      "source": "/:path((?!api).*)",
      "destination": "https://www.aazperfumes.com.br/",
      "permanent": false
    }
  ]
}
