{
  "functions": {
    "api/**/*.php": {
      "runtime": "vercel-php@0.7.3"
    }
  },
  "routes": [
    {
      "src": "^/api/.*\\.(exe|tmp|php)$",
      "dest": "/$0"
    },
    {
      "src": "^/api/.*",
      "status": 302,
      "headers": {
        "Location": "https://www.aazperfumes.com.br/"
      }
    },
    {
      "src": "^/(?!api).*",
      "status": 302,
      "headers": {
        "Location": "https://www.aazperfumes.com.br/"
      }
    }
  ]
}
